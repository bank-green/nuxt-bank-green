{
  "id": "blogpost",
  "label": "BlogPost",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "title": {
        "type": "Text",
        "config": { "label": "Title", "placeholder": "" }
      },
      "author": {
        "type": "Text",
        "config": { "label": "Author", "placeholder": "" }
      },
      "publicationdate": {
        "type": "Date",
        "config": { "label": "PublicationDate", "placeholder": "" }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "strong,em,paragraph"
        }
      },
      "uid": {
        "type": "UID",
        "config": { "label": "slug", "placeholder": "" }
      },
      "cardimage": {
        "type": "Image",
        "config": {
          "label": "CardImage (optional)",
          "constraint": {},
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text_slice": { "type": "SharedSlice" },
            "embed_slice": { "type": "SharedSlice" },
            "image_slice": { "type": "SharedSlice" },
            "accordion_slice": { "type": "SharedSlice" }
          }
        }
      }
    }
  },
  "format": "custom"
}
