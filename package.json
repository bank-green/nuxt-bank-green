{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "npx wrangler dev .output/server/index.mjs --site .output/public --local",
    "postinstall": "nuxt prepare",
    "clean": "nuxt clean",
    "slicemachine": "start-slicemachine",
    "publish": "npx wrangler publish",
    "stage": "npx wrangler publish --env staging",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:fix-changed": "git diff --name-only main | grep '\\.\\(ts\\|js\\|vue\\)$' | xargs eslint --fix",
    "lint:fix-changed-win": "powershell -Command \"git diff --name-only main | Where-Object {$_ -match '\\.(ts|js|vue)$'} | ForEach-Object {eslint --fix $_}\"",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@gtm-support/vue-gtm": "^2.0.0",
    "@nuxt/eslint": "^0.5.3",
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxt/test-utils": "^3.12.0",
    "@nuxtjs/prismic": "^3.0.1",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@prismicio/helpers": "^2.3.9",
    "@prismicio/types": "^0.2.4",
    "@slicemachine/adapter-nuxt": "^0.3.7",
    "@stripe/stripe-js": "^1.54.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@typeform/embed": "^2.8.0",
    "@vue/test-utils": "^2.4.5",
    "date-fns": "^2.29.3",
    "eslint": "^9.9.1",
    "fuse.js": "^6.6.2",
    "geonames.js": "^3.0.6",
    "happy-dom": "^13.10.1",
    "lottie-web": "^5.10.0",
    "nuxt": "^3.6.5",
    "playwright-core": "^1.43.0",
    "slice-machine-ui": "^1.6.0",
    "stripe": "^12.8.0",
    "typescript": "^5.5.4",
    "vite-plugin-eslint2": "^4.4.0",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "nuxt-jsonld": "^2.0.8",
    "vue-turnstile": "^1.0.9"
  }
}
