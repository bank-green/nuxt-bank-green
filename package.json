{
  "private": true,
  "engines": {
    "node": ">=22.19.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "npx wrangler dev .output/server/index.mjs --site .output/public --local",
    "postinstall": "nuxt prepare",
    "clean": "nuxt clean",
    "slicemachine": "start-slicemachine",
    "deploy": "npx wrangler deploy",
    "stage": "npx wrangler deploy -e staging",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "npx lint-staged",
    "lint-staged:fix": "npx lint-staged --fix",
    "lint:fix-changed-win": "powershell -Command \"git diff --name-only main | Where-Object {$_ -match '\\.(ts|js|vue)$'} | ForEach-Object {eslint --fix $_}\"",
    "test": "vitest --run",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@gtm-support/vue-gtm": "^2.0.0",
    "@nuxt/eslint": "^0.5.3",
    "@nuxt/image": "^1.0.0-rc.1",
    "@nuxt/test-utils": "^3.15.1",
    "@nuxtjs/prismic": "^3.0.1",
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@prismicio/helpers": "^2.3.9",
    "@prismicio/types": "^0.2.4",
    "@slicemachine/adapter-nuxt": "^0.3.7",
    "@stefanobartoletti/nuxt-social-share": "^1.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/vue": "^8.1.0",
    "@typeform/embed": "^2.8.0",
    "@vue/test-utils": "^2.4.5",
    "date-fns": "^2.29.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "fuse.js": "^6.6.2",
    "geonames.js": "^3.0.6",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lottie-web": "^5.10.0",
    "nuxt": "^3.14.159",
    "nuxt-graphql-client": "^0.2.43",
    "playwright-core": "^1.43.0",
    "prettier": "^3.5.3",
    "slice-machine-ui": "^2.11.0",
    "typescript": "^5.5.4",
    "vite-plugin-eslint2": "^4.4.0",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "^7.0.0",
    "@prismicio/helpers": "^2.0.0",
    "axios": "^1.7.9",
    "lock": "^1.1.0",
    "nuxt-jsonld": "^2.0.8",
    "rollup": "^4.28.1",
    "swiper": "^11.1.14",
    "vue-turnstile": "^1.0.9"
  },
  "lint-staged": {
    "*": "eslint"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.28.1"
  }
}